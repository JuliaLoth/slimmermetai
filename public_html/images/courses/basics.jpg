    <line x1="120" y1="150" x2="280" y2="150" stroke="#5852f2" stroke-width="2"/>
    
    <line x1="160" y1="200" x2="120" y2="150" stroke="#5852f2" stroke-width="2"/>
    <line x1="160" y1="200" x2="200" y2="230" stroke="#5852f2" stroke-width="2"/>
    
